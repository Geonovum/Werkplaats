                        <Begrippenlijst eId="cmp_o_1__content_o_1__list_o_1"
                                        wId="pv27_1__cmp_o_1__content_o_1__list_o_1">
                           <Begrip eId="cmp_o_1__content_o_1__list_o_1__item_o_1"
                                   wId="pv27_1__cmp_o_1__content_o_1__list_o_1__item_o_1"><!--voorbeschermd_gemeentelijk_monument.gml-->
                              <Term>Voorbeschermd gemeentelijk monument</Term>
                              <Definitie>
                                 <Al>
                                    <ExtIoRef eId="cmp_o_1__content_o_1__list_o_1__item_o_1__ref_o_1"
